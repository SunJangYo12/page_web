<h4>
IntentService for making Web Service requests - MyWebService.java</h4>
<pre class=3D"brush: java">package com.as400samplecode;
=20
import java.io.ByteArrayOutputStream;
import java.io.IOException;
=20
import org.apache.http.HttpResponse;
import org.apache.http.HttpStatus;
import org.apache.http.StatusLine;
import org.apache.http.client.ClientProtocolException;
import org.apache.http.client.HttpClient;
import org.apache.http.client.methods.HttpGet;
import org.apache.http.conn.params.ConnManagerParams;
import org.apache.http.impl.client.DefaultHttpClient;
import org.apache.http.params.HttpConnectionParams;
import org.apache.http.params.HttpParams;

import com.as400samplecode.MainActivity.MyWebReceiver;
=20
import android.app.IntentService;
import android.content.Intent;
import android.util.Log;
=20
public class MyWebService  extends IntentService{
=20
 private static final String LOG_TAG =3D "MyWebService";
    public static final String REQUEST_STRING =3D "myRequest";
    public static final String RESPONSE_STRING =3D "myResponse";
    public static final String RESPONSE_MESSAGE =3D "myResponseMessage";
=20
    private String URL =3D null;
    private static final int REGISTRATION_TIMEOUT =3D 3 * 1000;
    private static final int WAIT_TIMEOUT =3D 30 * 1000;
=20
    public MyWebService() {
        super("MyWebService");
    }
=20
    @Override
    protected void onHandleIntent(Intent intent) {
=20
        String requestString =3D intent.getStringExtra(REQUEST_STRING);
        Log.v(LOG_TAG, requestString);
        String responseMessage =3D "";
       =20
        try {
=20
            URL =3D requestString;
            HttpClient httpclient =3D new DefaultHttpClient();
            HttpParams params =3D httpclient.getParams();
=20
            HttpConnectionParams.setConnectionTimeout(params, REGISTRATION_=
TIMEOUT);
            HttpConnectionParams.setSoTimeout(params, WAIT_TIMEOUT);
            ConnManagerParams.setTimeout(params, WAIT_TIMEOUT);
=20
            HttpGet httpGet =3D new HttpGet(URL);
            HttpResponse response =3D httpclient.execute(httpGet);
=20
            StatusLine statusLine =3D response.getStatusLine();
            if(statusLine.getStatusCode() =3D=3D HttpStatus.SC_OK){
                ByteArrayOutputStream out =3D new ByteArrayOutputStream();
                response.getEntity().writeTo(out);
                out.close();
                responseMessage =3D out.toString();
            }
=20
            else{
                Log.w("HTTP1:",statusLine.getReasonPhrase());
                response.getEntity().getContent().close();
                throw new IOException(statusLine.getReasonPhrase());
            }
=20
        } catch (ClientProtocolException e) {
            Log.w("HTTP2:",e );
            responseMessage =3D e.getMessage();
        } catch (IOException e) {
            Log.w("HTTP3:",e );
            responseMessage =3D e.getMessage();
        }catch (Exception e) {
            Log.w("HTTP4:",e );
            responseMessage =3D e.getMessage();
        }
=20
=20
        Intent broadcastIntent =3D new Intent();
        broadcastIntent.setAction(MyWebReceiver.PROCESS_RESPONSE);
        broadcastIntent.addCategory(Intent.CATEGORY_DEFAULT);
        broadcastIntent.putExtra(RESPONSE_MESSAGE, responseMessage);
        sendBroadcast(broadcastIntent);
=20
    }
=20
}
