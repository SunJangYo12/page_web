;<br>&nbsp; <br>&nbsp; int phone_number[] =3D { <br>&nbsp; &nbsp; 1, 2, 3, =
4, <br>&nbsp; &nbsp; 5, 6, 7, 8,<br>&nbsp; &nbsp; 9, 0, 10, 11, <br>&nbsp; =
&nbsp; 12, 13, 14, 15<br>&nbsp; &nbsp; };<br><br>&nbsp; int i;<br>&nbsp; fo=
r(i =3D 0; i &lt; (sizeof(phone_number)/sizeof(int)); i ++) {<br>&nbsp; &nb=
sp; Serial.print(phone_number[i], 10);<br>&nbsp; &nbsp; Serial.print(' ');E=
<br>&nbsp; &nbsp; tone(2, _freq1[phone_number[i]], 500);<br>&nbsp; &nbsp; t=
one(3, _freq2[phone_number[i]], 500);<br>&nbsp; &nbsp; delay(550);<br>&nbsp=
; }<br><br>&nbsp; Serial.println();<br>}</code></pre></div><br><br>referenc=
