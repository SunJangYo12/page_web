<pre class=3D"brush: php; title: fileUpload.php; notranslate" title=3D"file=
Upload.php">
&lt;?php

// Path to move uploaded files
$target_path =3D "uploads/";

// array for final json respone
$response =3D array();

// getting server ip address
$server_ip =3D gethostbyname(gethostname());

// final file url that is being uploaded
$file_upload_url =3D 'http://' . $server_ip . '/' . 'AndroidFileUpload' . '=
/' . $target_path;


if (isset($_FILES['image']['name'])) {
    $target_path =3D $target_path . basename($_FILES['image']['name']);

    // reading other post parameters
    $email =3D isset($_POST['email']) ? $_POST['email'] : '';
    $website =3D isset($_POST['website']) ? $_POST['website'] : '';

    $response['file_name'] =3D basename($_FILES['image']['name']);
    $response['email'] =3D $email;
    $response['website'] =3D $website;

    try {
        // Throws exception incase file is not being moved
        if (!move_uploaded_file($_FILES['image']['tmp_name'], $target_path)=
) {
            // make error flag true
            $response['error'] =3D true;
            $response['message'] =3D 'Could not move the file!';
        }

        // File successfully uploaded
        $response['message'] =3D 'File uploaded successfully!';
        $response['error'] =3D false;
        $response['file_path'] =3D $file_upload_url . basename($_FILES['ima=
ge']['name']);
    } catch (Exception $e) {
        // Exception occurred. Make error flag true
        $response['error'] =3D true;
        $response['message'] =3D $e-&gt;getMessage();
    }
} else {
    // File parameter is missing
    $response['error'] =3D true;
    $response['message'] =3D 'Not received any file!F';
}

// Echo final json response to client
echo json_encode($response);
